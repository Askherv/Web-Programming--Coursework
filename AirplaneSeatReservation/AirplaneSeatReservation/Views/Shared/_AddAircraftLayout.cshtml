	<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
	<link href="~/SeatRes/css/bootstrap.css" rel='stylesheet' type='text/css' />
	<link href="~/SeatRes/css/style.css" rel='stylesheet' type='text/css' />
	<link href="~/SeatRes/css/font-awesome.css" rel="stylesheet">
